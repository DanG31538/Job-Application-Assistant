{
  "Login": {
    "URL": "https://www.linkedin.com/login",
    "Email/Username Input": {
      "Identifier": "#username"
    },
    "Password Input": {
      "Identifier": "#password"
    },
    "Sign in Button": {
      "Identifier": "button[type='submit']"
    }
  },
  "Job Search": {
    "URL": "https://www.linkedin.com/jobs/",
    "All Filters Button": {
      "Identifier": "button[aria-label='Show all filters']"
    },
    "Easy Apply Toggle": {
      "Identifier": "input[data-artdeco-toggle-button='true']"
    },
    "Show Results Button": {
      "Identifier": "button[data-test-reusables-filters-modal-show-results-button='true']"
    },
    "Job Title Input": {
      "Identifier": ".search-global-typeahead__input"
    },
    "Magnifying Glass (Initialize Search Fields) Icon": {
      "Identifier": "label[for='jobs-search-box-keyword-id-ember214']"
    },
    "Location Input": {
      "Identifier": "#jobs-search-box-location-id-ember214"
    },
    "Initiate Search": {
      "Method": "Press \"Enter\""
    }
  },
  "Iterate Through Job Listings": {
    "Job Link": {
      "Identifier": "a.job-card-container__link.job-card-list__title"
    }
  },
  "Apply for Job": {
    "Easy Apply Button": {
      "Identifier": "button[data-job-id]"
    },
    "Form Fields": {
      "Email Address": {
        "Identifier": "select#text-entity-list-form-component-formElement"
      },
      "Phone Country Code": {
        "Identifier": "select#text-entity-list-form-component-formElement-phoneNumber-country"
      },
      "Mobile Phone Number": {
        "Identifier": "input#single-line-text-form-component-formElement-phoneNumber-nationalNumber"
      },
      "Next Button on Easy Apply Form": {
        "Identifier": "button[aria-label='Continue to next step']"
      },
      "Submit Application": {
        "Identifier": "button[aria-label='Submit application']"
      }
    }
  }
}